{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"moduleResolution": "node"
		"outDir": "dist",
		"sourceMap": true,
		"baseUrl": "src",
		"strict": true,
		"paths": {
			"@/*": ["*"]
		},
		"lib": ["esnext"]
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "dist"]
}
